# Database Configuration
DB_USER=your_db_user
DB_HOST=localhost
DB_NAME=vamo_db
DB_PASSWORD=your_db_password
DB_PORT=5432

# Google Places API Key
GOOGLE_API_KEY=your_google_places_api_key_here

# Server Configuration
PORT=5001

# API Keys for existing services
# NUMVERIFY_API_KEY=your_numverify_api_key (deprecated - no longer used)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token

# Email Configuration (for admin authentication)
# Option 1: Resend (Recommended - works on Railway)
RESEND_API_KEY=re_xxxxxxxxxxxx
RESEND_FROM_EMAIL=onboarding@resend.dev

# Option 2: Gmail (deprecated - blocked by Railway)
# EMAIL_USER=your_email@gmail.com
# EMAIL_PASSWORD=your_app_password_here

# Admin email address
ADMIN_EMAIL=admin@vamo.sn

# Cloudinary Configuration (for image uploads)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret